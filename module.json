{
  "id": "terrainmapper",
  "title": "Terrain Mapper",
  "description": "Paint coded terrain values on Foundry scene maps.",
  "version": "#{VERSION}#",
  "library": "false",
  "socket": "true",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": "12",
    "verified": "12.327"
  },
  "authors": [
    {
      "name": "caewok",
      "url": "https://github.com/caewok",
      "discord": "caewok"
    }
  ],
  "relationships": {
    "systems": [],
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/ruipin/fvtt-lib-wrapper/master/module.json"
      },

      {
        "id": "socketlib",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-socketlib/master/module.json",
        "compatibility": {
          "verified": "1.0.13"
        }
      }
    ]
  },
  "esmodules": [
    "scripts/module.js"
  ],

  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],

  "documentTypes": {
    "RegionBehavior": {
      "setElevation": {},
      "setTerrain": {}
    }
  },

  "styles": [
    "styles/terrainlayer.css"
  ],

  "packs": [
    {
      "name": "terrainmapper_items_sfrpg",
      "label": "Terrain Mapper Items",
      "path": "packs/items/sfrpg",
      "type": "Item",
      "private": false,
      "system": "sfrpg"
    },
    {
      "name": "terrainmapper_items_pf2e",
      "label": "Terrain Mapper Items",
      "path": "packs/items/pf2e",
      "type": "Item",
      "private": false,
      "system": "pf2e"
    }
  ],

  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md"
}
